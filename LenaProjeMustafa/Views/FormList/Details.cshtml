@model List<Entitiess.Models.FormList>

@{
    ViewBag.Title = "Açıklama ";
}

<h2>Açıklama</h2>
<hr />
<div>
    
    <form role="form">
        <div class="form-group float-label-control">
            <label for="">Ad</label>
            <input type="text" class="form-control" value="@Model.FirstOrDefault().UserName">
        </div>
        <div class="form-group float-label-control">
            <label for="">Soyad</label>
            <input type="text" class="form-control" value="@Model.FirstOrDefault().UserSurname">
        </div>
        <div class="form-group float-label-control">
            <label for="">Yaş</label>
            <input type="text" class="form-control" value="@Model.FirstOrDefault().UserAge">
        </div>
        <div class="form-group float-label-control">
            <button type="submit" class="btn btn-outline-primary">Kaydet</button>
        </div>
        
    </form>
</div>
<p>
    @Html.ActionLink("Form Liste Sayfasına Geri Dön", "Index")
</p>
